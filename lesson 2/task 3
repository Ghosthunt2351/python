n = int(input('Введите номер месяца: '))
months_list = ['Январь', 'Февраль', 'Март', 'Апрель', 'Май', 'Июнь', 'Июль', 'Август', 'Сентябрь', 'Октябрь', 'Ноябрь', 'Декабрь']
months_dict = {1: 'Январь', 2: 'Февраль', 3: 'Март', 4: 'Апрель', 5: 'Май', 6: 'Июнь', 7: 'Июль', 8: 'Август', 9: 'Сентябрь', 10: 'Октябрь', 11: 'Ноябрь', 12: 'Декабрь'}
print(months_list[n-1])
print(months_dict[n])
seasons = {1: 'Зима', 2: 'Зима', 3: 'Весна', 4: 'Весна', 5: 'Весна', 6: 'Лето', 7: 'Лето', 8: 'Лето', 9: 'Осень', 10: 'Осень', 11: 'Осень', 12: 'Зима'}
seasons_dict = {'Январь': 'Зима', 'Февраль': 'Зима', 'Март': 'Весна', 'Апрель': 'Весна', 'Май': 'Весна', 'Июнь': 'Лето', 'Июль': 'Лето', 'Август': 'Лето', 'Сентябрь': 'Осень', 'Октябрь': 'Осень', 'Ноябрь': 'Осень', 'Декабрь': 'Зима'}
print(seasons[n])
print(seasons_dict[months_list[n-1]])
