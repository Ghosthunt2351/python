from abc import ABC, abstractmethod


class Clothes(ABC):
    def __init__(self, v, h):
        self.v = v
        self.h = h

    @abstractmethod
    def rashod(self):
        pass


class Coat(Clothes):
    @property
    def rashod(self):
        return self.v / 6.5 + 0.5


class Costume(Clothes):
    @property
    def rashod(self):
        return 2 * self.h + 0.3


a = Coat(46, 32)
print(f'Расход ткани на пошив пальто: {a.rashod}')
b = Costume(50, 30)
print(f'Расход ткани на пошив костюму: {b.rashod}')
print(f'Общий расход ткани на пошив одежды: {a.rashod + b.rashod}')
