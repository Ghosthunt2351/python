class Road:
    def __init__(self, length, width):
        self._length = length
        self._width = width

    def mass(self):
        return self._length * self._width * 25 * 5 / 1000


x = int(input('Введите длину вашей дороги: '))
y = int(input('Введите ширину вашей дороги: '))
my_road = Road(x, y)
print(my_road.mass())
